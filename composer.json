{
    "name": "themonkeys/laravel-google-auth",
    "description": "Laravel Authentication Driver for Google OAuth authentication",
    "keywords": ["laravel", "auth", "authentication", "oauth2", "google", "login"],
    "authors": [
        {
            "name": "The Monkeys",
            "email": "developers@themonkeys.com.au"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.0",
        "google/google-api-php-client": "0.6.x"
    },
    "autoload": {
        "psr-0": {
            "Themonkeys\\LaravelGoogleAuth": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "google/google-api-php-client",
                "version": "0.6.7",
                "dist": {
                    "url": "https://s3.amazonaws.com/haveeru.servers/google-api-php-client.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["src/"]
                }
            }
        }
    ],
    "minimum-stability": "dev"
}
